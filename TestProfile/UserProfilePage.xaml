<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TestProfile.UserProfilePage"
             xmlns:viewmodels="clr-namespace:TestProfile.ViewModel"
             x:DataType="viewmodels:UserProfileViewModel"
             Title="UserProfilePage">
    <Shell.BackButtonBehavior>
        <BackButtonBehavior IsVisible="False" IsEnabled="False" />
    </Shell.BackButtonBehavior>
    <VerticalStackLayout>
        <Label 
            Text="Welcome to .NET MAUI!"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <Button Text="Go To Login"
                Command="{Binding GoToLoginCommand}"></Button>
    </VerticalStackLayout>
</ContentPage>