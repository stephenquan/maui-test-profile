<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="TestProfile.UserProfilePage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:viewmodels="clr-namespace:TestProfile.ViewModel"
    Title="UserProfilePage"
    x:DataType="viewmodels:UserProfileViewModel">
    <VerticalStackLayout>
        <Label
            HorizontalOptions="Center"
            Text="Welcome to .NET MAUI!"
            VerticalOptions="Center" />
        <Button Command="{Binding GoToLoginCommand}" Text="Go To Login" />
    </VerticalStackLayout>
</ContentPage>