<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="TestProfile.LoginPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:viewmodels="clr-namespace:TestProfile.ViewModel"
    Title="LoginPage"
    x:DataType="viewmodels:LoginViewModel">
    <VerticalStackLayout>
        <Label
            HorizontalOptions="Center"
            Text="Hello Login"
            VerticalOptions="Center" />
        <Button Command="{Binding GoToProfileCommand}" Text="Go To User" />
        <Button Command="{Binding BackToMainCommand}" Text="Back To Main" />
    </VerticalStackLayout>
</ContentPage>